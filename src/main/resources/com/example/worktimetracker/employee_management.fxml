<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.worktimetracker.Employee_managementController">
    <children>
      <Button layoutX="446.0" layoutY="454.0" mnemonicParsing="false" onAction="#openAddNewEmployeeWindow" text="사원 추가하기" />
      <TableView fx:id="employeeTableView" layoutX="43.0" layoutY="34.0" onSort="#initialize" prefHeight="400.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="90.0" text="이름" />
          <TableColumn fx:id="birthColumn" prefWidth="100.0" text="생년월일" />
            <TableColumn fx:id="addressColumn" prefWidth="100.0" text="주소" />
            <TableColumn fx:id="employeedateColumn" prefWidth="100.0" text="입사 날짜" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="100.0" text="전화번호" />
        </columns>
      </TableView>
      <Button fx:id="editEmplyeeButton" layoutX="334.0" layoutY="454.0" mnemonicParsing="false" onAction="#openEditEmployeeWindow" text="정보 수정하기" />
    </children>
</AnchorPane>
